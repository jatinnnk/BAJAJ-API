{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/bfhl",
            "dest": "/api/bfhl.js"
        },
        {
            "src": "/health",
            "dest": "/api/health.js"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}